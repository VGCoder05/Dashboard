{
  "version": 2,
  "build": {
    "env": {
      "NODE_VERSION": "18"
    }
  },
  "builds": [
    { "src": "client/package.json", "use": "@vercel/static-build" },
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "client/$1" }
  ]
}
